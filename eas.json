{
  "cli": {
    "version": ">= 14.0.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "android": {
        // "autoIncrement": true,
        "releaseStatus": "draft",
        "changesNotSentForReview": true,
        // (enum: production, beta, alpha, internal) 
        "track": "internal"
      }
    }
  }
}